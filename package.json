{
  "name": "find-phone-country",
  "version": "4.1.2",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "tsc": "tsc",
    "test": "jest",
    "start": "react-native start",
    "clean_android": "cd android && rm -rf app/build/* && rm -rf app/release/* && rm -rf app/debug/* && rm -rf build/* && (./gradlew --stop || true) && rm -rf ~/.gradle/caches/ && ./gradlew clean && cd ..",
    "hard_clean_android": "(watchman watch-del-all || true) && rm -rf node_modules/ && npm i && rm -rf /tmp/metro-* && npm run clean_android && npm run start --reset-cache",
    "build_android_apk": "cd android && ./gradlew assembleRelease && cd ..",
    "build_android_aab": "cd android && ./gradlew bundleRelease && cd ..",
    "analyze_bundle": "npx react-native-bundle-visualizer --platform android",
    "dev_release": "react-native run-android --variant=release",
    "dev": "react-native run-android",
    "dev_linux": "concurrently \"npm run start\" \"npm run dev\""
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/masked-view": "^0.1.10",
    "dayjs": "^1.10.4",
    "native-base": "^2.15.2",
    "react": "17.0.2",
    "react-native": "0.64.1",
    "react-native-action-sheet": "^2.2.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.18.1",
    "react-native-select-contact": "^1.5.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-timeline-flatlist": "^0.7.3",
    "react-native-vector-icons": "^8.1.0",
    "react-navigation": "^4.4.4",
    "react-navigation-drawer": "^2.7.0",
    "react-navigation-stack": "^2.10.4",
    "react-navigation-tabs": "^2.11.0"
  },
  "devDependencies": {
    "@babel/core": "7.13.15",
    "@babel/runtime": "7.13.10",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.3",
    "@types/react-native": "^0.64.2",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "26.6.3",
    "concurrently": "^6.0.1",
    "eslint": "^7.24.0",
    "eslint-plugin-react": "^7.23.2",
    "jest": "26.6.3",
    "metro-config": "^0.65.2",
    "metro-react-native-babel-preset": "^0.65.2",
    "react-native-console-time-polyfill": "^1.2.3",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.2.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
